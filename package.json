{
  "name": "simple-git",
  "description": "Simple GIT interface for node.js",
  "version": "2.1.0",
  "author": "Steve King <steve@mydev.co>",
  "contributors": [
    {
      "name": "Steve King",
      "email": "steve@mydev.co"
    }
  ],
  "dependencies": {
    "@kwsites/exec-p": "^0.4.0",
    "debug": "^4.0.1",
    "diff2html": "^3.1.6"
  },
  "devDependencies": {
    "@kwsites/jestify-node-unit": "^1.0.1",
    "@types/jest": "^25.2.1",
    "@types/node": "^13.11.1",
    "jest": "^25.3.0",
    "sinon": "^7.3.2",
    "ts-jest": "^25.4.0",
    "ts-node": "^8.8.2",
    "typescript": "^3.8.3"
  },
  "keywords": [
    "git",
    "source control",
    "vcs"
  ],
  "license": "MIT",
  "repository": "git://github.com/steveukx/git-js.git",
  "main": "./src/index.js",
  "files": [
    "promise.*",
    "src/**/*.d.ts",
    "src/**/*.js",
    "src/**/*.js.map",
    "typings/"
  ],
  "scripts": {
    "clean": "git clean -fX",
    "tsc": "tsc",
    "build": "tsc --build",
    "pretest": "yarn build",
    "test": "jest --coverage",
    "preversion": "yarn test",
    "postversion": "npm publish && git push && git push --tags"
  },
  "jest": {
    "testMatch": [
      "**/test/**/test-*.js",
      "**/test/**/*.spec.js"
    ]
  }
}
